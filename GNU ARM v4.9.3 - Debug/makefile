# Tool invocations
sheep.axf: $(OBJS) $(USER_OBJS)
	@echo 'Building target: $@'
	@echo 'Invoking: GNU ARM C Linker'
	arm-none-eabi-gcc -g -gdwarf-2 -mcpu=cortex-m0plus -mthumb -T "sheep.ld" -Xlinker --gc-sections -Xlinker -Map="sheep.map" --specs=nano.specs -o sheep.axf "./src/displaygraphics.o" "./src/humitemp.o" "./src/lsm_9DS1.o" "./emlib/em_adc.o" "./emlib/em_assert.o" "./emlib/em_cmu.o" "./emlib/em_core.o" "./emlib/em_emu.o" "./emlib/em_gpio.o" "./emlib/em_i2c.o" "./emlib/em_leuart.o" "./emlib/em_rtc.o" "./emlib/em_usart.o" "./Graphics/dmd_display.o" "./Graphics/glib.o" "./Graphics/glib_bitmap.o" "./Graphics/glib_circle.o" "./Graphics/glib_font_narrow_6x8.o" "./Graphics/glib_font_normal_8x8.o" "./Graphics/glib_line.o" "./Graphics/glib_rectangle.o" "./Graphics/glib_string.o" "./Drivers/display.o" "./Drivers/displayls013b7dh03.o" "./Drivers/displaypalemlib.o" "./Drivers/i2cspm.o" "./Drivers/rtcdriver.o" "./Drivers/si7013.o" "./Drivers/udelay.o" "./CMSIS/EFM32HG/startup_gcc_efm32hg.o" "./CMSIS/EFM32HG/system_efm32hg.o" "./BSP/bsp_bcc.o" "./BSP/bsp_stk.o" "./BSP/bsp_stk_leds.o" -Wl,--start-group -lgcc -lc -lnosys -Wl,--end-group
	@echo 'Finished building target: $@'
	@echo ' '

	@echo 'Building hex file: sheep.hex'
	arm-none-eabi-objcopy -O ihex "sheep.axf" "sheep.hex"
	@echo ' '

	@echo 'Building bin file: sheep.bin'
	arm-none-eabi-objcopy -O binary "sheep.axf" "sheep.bin"
	@echo ' '

	@echo 'Building s37 file: sheep.s37'
	arm-none-eabi-objcopy -O srec "sheep.axf" "sheep.s37"
	@echo ' '

	@echo 'Running size tool'
	arm-none-eabi-size "sheep.axf"
	@echo ' '

